
<%= form_for @treatment_plan,  treatment_plan_form_options(@treatment_plan) do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name, required: true %>
  </p>

  <%= f.hidden_field :patient_id, value: @treatment_plan.patient.id %>

  <%= f.submit %>
<% end %>


