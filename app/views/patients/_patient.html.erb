<%= patient.name %><br>
<%= patient.dob %><br>
<%= patient.phone_number %><br>
<%= patient.address %><br><br>

<%= link_to "Edit", edit_patient_path(patient), class: "right-margin-btn btn btn-primary btn-sm" %>

<%= link_to "Delete", patient, method: :delete, data: { confirm: "Are you sure you would like to delete this patient? This will also delete the patient's appointments." }, class: "delete-patient right-margin-btn btn btn-primary btn-sm" %><br><br>
