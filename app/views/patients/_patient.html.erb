<table class="w3-table">
<tr>
  <td>Name</td>
  <td><%= patient.name %></td>
</tr>
<tr>
  <td>DOB</td>
  <td><%= date_of_birth(patient) %></td>
</tr>
<tr>
  <td>Phone</td>
  <td><%= patient.phone_number %></td>
</tr>
<tr>
  <td>Address</td>
  <td><%= patient.address %></td>
</tr>
</table>
<%= link_to "Edit", edit_patient_path(patient), class: "right-margin-btn btn btn-primary btn-sm" %>
