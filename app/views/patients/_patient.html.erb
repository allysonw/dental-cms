<table class="w3-table">
<tr>
  <td>DOB</td>
  <td><%= date_of_birth(patient) %></td>
</tr>
<tr>
  <td>Phone</td>
  <td><%= patient.phone_number %></td>
</tr>
<tr>
  <td>Address</td>
  <td>
    <%= patient.address.street_1 %><br>
    <%= patient.address.street_2 %><br>
    <%= patient.address.city %>,
    <%= patient.address.state %><br>
    <%= patient.address.zip_code %>
  </td>
</tr>
<tr>
  <td><%= link_to "Edit", edit_patient_path(patient) %></td>
</tr>
</table>
